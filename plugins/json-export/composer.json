{
    "name": "thisisablock/craft-json-export",
    "type": "craft-plugin",
    "require": {
        "php": ">=8.0.0",
        "craftcms/cms": "^4.4.7.1"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "JsonExport\\": "src/"
        }
    },
    "extra": {
        "handle": "json-export",
        "name": "JsonExport",
        "developer": "thisisablock",
        "documentationUrl": "",
        "class": "JsonExport\\JsonExport"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

